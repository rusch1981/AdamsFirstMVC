<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://adamsproject.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>adamsproject.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>AdamsProject</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$AdamsProject</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="MandMContext" Order="1" Enabled="True">
          <Destination Path="Data Source=mandmserve1.database.windows.net;Initial Catalog=MandMWebDB1;Persist Security Info=True;User ID=mandmadmin;Password=Enterprize1" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="AdamsFirstMVC.DAL.MandMContext, AdamsFirstMVC" MigrationConfiguration="AdamsFirstMVC.Migrations.Configuration, AdamsFirstMVC" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)MandMContext-Web.config Connection String">
      <ParameterValue>Data Source=mandmserve1.database.windows.net;Initial Catalog=MandMWebDB1;Persist Security Info=True;User ID=mandmadmin;Password=Enterprize1</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>